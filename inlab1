$ xdg-open Lab.pdf      #open with default application 
$ script filename.script      #started saving works to filename.script 
$ exit              #script is done file is saved 
$ touch lab1_1.txt lab1_2.doc lab1_3.odt   #create files 
$ mkdir -p assignment1/lab1      #create directories one inside another 
$ cd  nextDir     $ cd ..     $cd       #navigate forward backward and home commond 
$ cat iit.txt  >>lab1_1.txt >>lab1_2.doc >>lab1_3.odt     #copying content
$ ls *.txt *.doc *.odt        #list only specific files 
$ pwd         # displays full path of present working directory 
$ cat > commands.txt          # create new file with editing option 
$ chmod +rwx commands.txt      #making read write and executable file 
$ head -11 filename     #dispalys first 11 lines of file 
$ tail -7 filename      #displays last 7 lines of file
$ cp filename  destinationDir     #copying file to another direcctory 
$ mv filename  destinationDir      #moving file to another direcctory
$ grep assignment lab1_1.txt       #searching for string match in given file 
$ grep -w assignment1 lab1_1.txt   #for specific word 
$ grep -x "exact matching line" lab1_1.txt   #finding exact match of line 
$ grep -r  -w   wordToSearch *          #recursive search to all directories and files 
$ grep -c word  filename     #counts the number of occurance 

